{% extends 'base.html.twig' %}
{% block body %}
    <div class="row">
        <br/>
    </div>
    <div class="row">
        <div class="col s6">
            <img class="img-about" src="{{ asset('materializecss/images/background1.jpg') }}"/>
        </div>
        <div class="col s6 min-height">
            <h4>About Us</h4>
            <ul class="collapsible popout" data-collapsible="accordion">
                {% for a in about %}
                    <li>
                        <div class="collapsible-header">{{ a.title }}</div>
                        <div class="collapsible-body"><p>{{ a.content }}</p></div>
                    </li>
                {% endfor %}
            </ul>
        </div>
    </div>
{% endblock %}